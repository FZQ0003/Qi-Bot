{
  "$schema": "http://json-schema.org/draft-07/schema",
  "title": "File Config",
  "description": "文件相关配置",
  "type": "object",
  "properties": {
    "cache": {
      "description": "缓存文件设置",
      "type": "object",
      "properties": {
        "enable": {
          "description": "是否启用缓存文件",
          "type": "boolean",
          "default": true
        },
        "max_count": {
          "description": "缓存文件最大数量，初始化时清理",
          "type": "integer",
          "minimum": -1,
          "default": -1
        },
        "expire_days": {
          "description": "缓存文件保留天数，初始化时清理",
          "type": "integer",
          "minimum": 0,
          "default": 30
        }
      }
    }
  }
}
